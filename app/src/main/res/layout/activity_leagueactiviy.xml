<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nextpagebbtn2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".leagueactiviy">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:contentDescription="todo"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/desiredleaguebg" />

    <ImageView
        android:id="@+id/swooshlogo"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginBottom="668dp"
        android:contentDescription="todo"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/swooshlogo"
        tools:ignore="UnknownIdInLayout" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat"
        android:text="campionato desiderato"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.151" />

    <ToggleButton
        android:id="@+id/donnebutton"
        android:layout_width="320dp"
        android:layout_height="52dp"
        android:layout_marginTop="24dp"
        android:background="@android:drawable/gallery_thumb"
        android:fontFamily="@font/montserrat"
        android:textColor="@color/white"
        android:textOff="donne"
        android:textOn="donne"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/uominibtn" />

    <ToggleButton
        android:id="@+id/uominibtn"
        android:layout_width="320dp"
        android:layout_height="52dp"
        android:background="@android:drawable/gallery_thumb"
        android:fontFamily="@font/montserrat"
        android:textColor="@color/white"
        android:textOff="uomini"
        android:textOn="uomini"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.239" />

    <ToggleButton
        android:id="@+id/coedbutton"
        android:layout_width="320dp"
        android:layout_height="52dp"
        android:layout_marginTop="24dp"
        android:background="@android:drawable/gallery_thumb"
        android:fontFamily="@font/montserrat"
        android:textColor="@color/white"
        android:textOff="co_ed"
        android:textOn="co_ed"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/donnebutton" />

    <Button
        android:id="@+id/nextpagebtn3"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="380dp"
        android:layout_height="52dp"
        android:layout_marginBottom="16dp"
        android:background="@android:drawable/screen_background_dark_transparent"
        android:onClick="nextpage2"
        android:text="next"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>